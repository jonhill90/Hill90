# Tailscale Configuration Example
# Copy this file to terraform.tfvars and fill in your values
# DO NOT commit terraform.tfvars (it contains secrets)

tailscale_api_key  = "tskey-api-xxxxx"
tailscale_tailnet  = "your-tailnet.ts.net"
vps_hostname       = "hill90-vps"
enable_ssh         = true
enable_accept_routes = true
